{
  "name": "gulp-coffeelint",
  "version": "0.6.0",
  "description": "Lint your CoffeeScript using gulp and CoffeeLint",
  "keywords": [
    "gulpplugin",
    "lint",
    "coffee",
    "coffeelint",
    "coffeescript",
    "coffee-script",
    "codeconventions"
  ],
  "repository": "janraasch/gulp-coffeelint",
  "author": {
    "name": "Jan Raasch",
    "email": "jan@janraasch.com",
    "url": "http://janraasch.com"
  },
  "files": [
    "index.js",
    "lib/*.js"
  ],
  "scripts": {
    "prepublish": "gulp coffee",
    "test": "coffeelint gulpfile.coffee index.coffee lib test -f ./coffeelint.json && nyc --require coffeescript/register --extension .coffee mocha --report lcovonly --reporter spec -- ./test/*.coffee",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "changelog": "conventional-changelog -p angular -i changelog.md -w"
  },
  "dependencies": {
    "args-js": "^0.10.5",
    "coffeelint": "^2.0.6",
    "coffeelint-stylish": "^0.1.1",
    "plugin-error": "^1.0.1",
    "through2": "^3.0.0"
  },
  "devDependencies": {
    "coffeescope2": "^0.5.1",
    "coffeescript": "^2.3.2",
    "conventional-changelog": "^3.0.5",
    "coveralls": "^3.0.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-coffee": "^3.0.3",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "proxyquire": "^2.1.0",
    "should": "^13.2.3",
    "sinon": "^7.2.3",
    "vinyl": "^2.2.0"
  },
  "engines": {
    "npm": ">=1.3.7",
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
